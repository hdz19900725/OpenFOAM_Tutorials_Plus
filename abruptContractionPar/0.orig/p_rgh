FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "0";
    object      p_rgh;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [1 -1 -2 0 0];

internalField   uniform 72637;

boundaryField
{
    inlet
    {
        type            fixedValue;
        value           119860;	//experimental value
    }

    outlet
    {
        type            fixedValue;
        value           72637;	//experimental value
    }

    walls
    {
        type            fixedFluxPressure;
    }
    side
    {
        type            fixedFluxPressure;
    }
    symmetry
    {
        type            fixedFluxPressure;
    }
}

